{
    "name": "danielmmf/yii2-jwt",
    "description": "JWT based on sizeg",
	"type": "yii2-extension",
	"keywords": ["yii2", "yii 2", "jwt"],
	"authors": [
        {
            "name": "Daniel Medeiros",
            "email": "danielmmf@gmail.com",
            "homepage": "https://danielmmf.herokuapp.com"
        }
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.6.0",
        "lcobucci/jwt": "~3.3.0",
        "yiisoft/yii2": "~2.0.0"
    },
	"require-dev": {
		"phpunit/phpunit": "^4.8"
	},
    "autoload": {
        "psr-4": {
            "sizeg\\jwt\\": ""
        }
    },
	"autoload-dev": {
        "psr-4": {
            "sizeg\\jwt\\tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]    
}
